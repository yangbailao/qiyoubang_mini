/* 轮播图 */
.bannerView {
  padding: 0;

  z-index: 50;
  height: 300rpx;
  margin-bottom: 30rpx;
}
.swiper {
  position: relative;
  overflow: hidden;
}
.swiper swiper{
  height: 300rpx;
  /* height: 445rpx; */
}
.swiper swiper-item {
  /* padding: 0 7rpx; */
  box-sizing: border-box;
  width: 636rpx;
}
.banner-item {
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  color: #666;
  font-size: 26rpx;
}
.banner-item image {
  width: 100%;
  display: block;
}

.dots{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
  z-index: 99;
}
.dots .dot{
  margin: 0 5rpx;
  width: 10rpx;
  height: 10rpx;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 5rpx;
  transition: all .5s;
}
.dots .dot.active{
  width: 50rpx;
  background: #fff;
  transition: 0.5s all;
}

/* 广告图 */
.bannerImg image {
  width: 100%;
  display: block;
}

